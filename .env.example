# MongoDB Atlas connection string (REQUIRED)
# Get this from your MongoDB Atlas dashboard
MONGODB_URI=mongodb+srv://USER:PASSWORD@CLUSTER.mongodb.net/?retryWrites=true&w=majority
MONGODB_DB=app
GEMINI_API_KEY=your-gemini-api-key

# Tailscale Auth Key (OPTIONAL - leave blank if not using Tailscale)
# Generate an ephemeral key at: https://login.tailscale.com/admin/settings/keys
# DO NOT COMMIT this value to git - it should be in .env only
TS_AUTHKEY=tskey-auth-REPLACE_ME

# Vultr Object Storage (S3-compatible) - OPTIONAL
# Get these from Vultr Object Storage dashboard: https://my.vultr.com/objectstorage/
# Endpoint format: https://REGION.vultrobjects.com (e.g., https://ewr1.vultrobjects.com)
VULTR_ENDPOINT=https://REGION.vultrobjects.com
VULTR_ACCESS_KEY=your_access_key_here
VULTR_SECRET_KEY=your_secret_key_here
VULTR_BUCKET=your_bucket_name
